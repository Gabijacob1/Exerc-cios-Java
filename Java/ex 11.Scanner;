import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner cat = new Scanner(System.in);

        System.out.print("Digite o primeiro número: ");
        int n1 = cat.nextInt();

        System.out.print("Digite o segundo número: ");
        int n2 = cat.nextInt();

        if (n1 % n2 == 0 || n2 % n1 == 0) {
            System.out.println("Os números " + n1 + " e " + n2 + " são múltiplos um do outro.");
        } else {
            System.out.println("Os números " + n1 + " e " + n2 + " não são múltiplos um do outro.");
        }
    }
